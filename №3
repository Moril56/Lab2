#include <stdio.h>
#include <math.h> // Підключення Біблліотек

int main() {
    int x, y, z; //оголошення змінних
    double f;

    // Ввід значень з клавіатури
    printf("Введіть значення x: ");
    scanf("%d", &x);
    printf("Введіть значення y: ");
    scanf("%d", &y);
    printf("Введіть значення z: ");
    scanf("%d", &z);

    /* Обчислення значення функції 
    піднесенням числа до певного степення 
    використовуючи функцію pow
      */
    f = (pow(x, 2) + pow(z, 2)) / sqrt(x + y);// Використання квадратного кореня

    // Виведення результату на консоль
    printf("Значення функції f = %.0f\n", f);

    return 0;
}
